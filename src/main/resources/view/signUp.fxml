<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="750.0" prefWidth="1100.0"
          style="-fx-border-color: BLACK;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="viewmodel.SignUpController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="460.0" minWidth="10.0" prefWidth="162.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="666.0" minWidth="10.0" prefWidth="170.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="666.0" minWidth="10.0" prefWidth="170.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="252.0" minWidth="0.0" prefWidth="184.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="129.0" minHeight="10.0" prefHeight="75.39999923706054" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="129.0" minHeight="10.0" prefHeight="64.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="129.0" minHeight="10.0" prefHeight="23.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="250.0" minHeight="0.0" prefHeight="56.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="277.0" minHeight="10.0" prefHeight="59.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="277.0" minHeight="10.0" prefHeight="58.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="335.1999877929687" minHeight="10.0" prefHeight="48.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="335.1999877929687" minHeight="10.0" prefHeight="61.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="335.1999877929687" minHeight="10.0" prefHeight="61.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="335.1999877929687" minHeight="10.0" prefHeight="73.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="335.1999877929687" minHeight="10.0" prefHeight="170.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="335.1999877929687" minHeight="10.0" prefHeight="64.0" vgrow="SOMETIMES"/>
        <RowConstraints/>
    </rowConstraints>
    <children>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="6" GridPane.rowIndex="2">
            <children>
                <Label alignment="CENTER" focusTraversable="false" prefHeight="74.0" prefWidth="1099.0"
                       style="-fx-border-color: BLACK;" text="Sign Up New User Account">
                    <font>
                        <Font name="System Bold" size="36.0"/>
                    </font>
                </Label>
            </children>
        </HBox>

        <!-- Registration Form Section -->
        <VBox alignment="CENTER" spacing="10" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="8">
            <Label style="-fx-font-size: 18px;" text="Registration Form"/>

            <HBox alignment="CENTER" spacing="10">
                <Label text="First Name:"/>
                <TextField fx:id="firstNameField"/>
                <Label fx:id="firstNameCheck"/>
            </HBox>

            <HBox alignment="CENTER" spacing="10">
                <Label text="Last Name:"/>
                <TextField fx:id="lastNameField"/>
                <Label fx:id="lastNameCheck"/>
            </HBox>

            <HBox alignment="CENTER" spacing="10">
                <Label text="Email:"/>
                <TextField fx:id="emailField"/>
                <Label fx:id="emailCheck"/>
            </HBox>

            <HBox alignment="CENTER" spacing="10">
                <Label text="Password:"/>
                <PasswordField fx:id="passwordField"/>
                <Label fx:id="passwordCheck"/>
            </HBox>

            <HBox alignment="CENTER" spacing="10">
                <Label text="Confirm Password:"/>
                <PasswordField fx:id="confirmPasswordField"/>
                <Label fx:id="confirmPasswordCheck"/>
            </HBox>

            <HBox alignment="CENTER" spacing="10">
                <Label text="Date of Birth (MM/DD/YYYY):"/>
                <TextField fx:id="dobField"/>
                <Label fx:id="dobCheck"/>
            </HBox>

            <HBox alignment="CENTER" spacing="10">
                <Label text="Zip Code:"/>
                <TextField fx:id="zipCodeField"/>
                <Label fx:id="zipCodeCheck"/>
            </HBox>

            <Button fx:id="newAccountBtn" mnemonicParsing="false" onAction="#createNewAccount" prefHeight="43.0"
                    prefWidth="199.0" style="-fx-background-color: GREY; -fx-border-color: BLACK;"
                    text="Create New Account" textAlignment="CENTER" textFill="WHITE">
                <VBox.margin>
                    <Insets bottom="5.0" top="5.0"/>
                </VBox.margin>
                <font>
                    <Font name="System Bold" size="15.0"/>
                </font>
            </Button>

            <Button fx:id="goBackBtn" alignment="CENTER" mnemonicParsing="false" onAction="#goBack" prefHeight="40.0"
                    prefWidth="200.0" style="-fx-background-color: Gray; -fx-border-color: BLACK;" text=" Login"
                    textFill="WHITE">
                <font>
                    <Font name="System Bold" size="14.0"/>
                </font>
                <VBox.margin>
                    <Insets bottom="5.0" top="5.0"/>
                </VBox.margin>
            </Button>
        </VBox>
    </children>
</GridPane>
